<h1>Listing new_users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Work</th>
    <th>Time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @new_users.each do |new_user| %>
  <tr>
    <td><%= new_user.name %></td>
    <td><%= new_user.work %></td>
    <td><%= new_user.time %></td>
    <td><%= link_to 'Show', new_user %></td>
    <td><%= link_to 'Edit', edit_new_user_path(new_user) %></td>
    <td><%= link_to 'Destroy', new_user, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New New user', new_new_user_path %>
